{
	"name": "Volume Scroll",
	"description": "Scrollable Volume",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"storage"
	],
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"run_at": "document_start",
			"css": ["contentStyle.css"],
			"js": ["contentScript.js"]
		}
	]
}
